<body>



<div class="page-header section-dark " style=" background-attachment: fixed; background-image: url('https://zupimages.net/up/19/48/grvc.jpg')">
    <div class="content-center">
      <div class="container">
        <div class="text-center">
          <h1 class="title">Katchup</h1>
        </div>
        <h2 class="presentation-subtitle text-center">La faudra mettre une description de chat car il sont mignions quand meme</h2>
      </div>
    </div>
    </div>

  <div class="site-blocks-cover overlay" style="" data-aos="fade" data-stellar-background-ratio="0.5">
    <div class="container">
      <div class="row align-items-center justify-content-center text-center">
        <div class="col-md-10">
          <div class="row justify-content-center mb-4">
            <div class="col-md-8 text-center">
              <h1 class="title text-primary">Ajouts recents</h1>
              <p data-aos="fade-up" data-aos-delay="100">Explore top-rated attractions, activities and more!</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

<!-- Cards -->

<div class="card-deck text-center mx-auto" style="width: 80%;">
  <% @items.each_with_index do |t, index| %>
<%  if index < 5 %>
  <div class="card" style="">
  <img class="card-img-top" src="<%= t.image_url %>" alt="Card image cap">
  <div class="card-body">
    <h4 class="card-title"><%= t.title %></h4>
    <p class="card-text"></p>
    <a href="<%= item_path(t) %>" class="btn btn-primary">Voir plus </a>
  </div>
</div>
<% end %>
<% end %>
</div>



<!-- Cards Deck End  -->
<br><br>
